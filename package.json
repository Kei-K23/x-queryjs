{
    "version": "0.1.1",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "x-queryjs",
    "author": "Kei-K23",
    "module": "dist/x-queryjs.esm.js",
    "size-limit": [
        {
            "path": "dist/x-queryjs.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/x-queryjs.esm.js",
            "limit": "10 KB"
        }
    ],
    "lint-staged": {
        "*.{ts,tsx}": [
            "pnpm run test"
        ]
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^11.1.4",
        "@types/jest": "^29.5.12",
        "eslint": "^9.7.0",
        "husky": "^9.1.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.3",
        "size-limit": "^11.1.4",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.3",
        "typescript": "^3.9.10"
    }
}